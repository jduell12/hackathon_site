    "chartjs-plugin-datalabels": "^2.2.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^10.8.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.1",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@pendo/eslint-config": "^2.0.1",
    "@pendo/eslint-plugin": "^1.6.0",
    "@rushstack/eslint-patch": "^1.8.0",
